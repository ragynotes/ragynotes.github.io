  author = {Cinosi, Eduardo and Martinotti, Giovanni and Simonato, Pierluigi and Singh, Darshan and Demetrovics, Zsolt and {Roman-Urrestarazu}, Andres and Bersani, Francesco Saverio and Vicknasingam, Balasingam and Piazzon, Giulia and Li, Jih-Heng and Yu, Wen-Jing and {Kapit{\'a}ny-F{\"o}v{\'e}ny}, M{\'a}t{\'e} and Farkas, Judit and Di Giannantonio, Massimo and Corazza, Ornella},
